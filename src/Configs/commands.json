						{
							"Name": "Bot?",
							"Value": "{isBot}",
							"Inline": true
						},
						{
							"Name": "Admin?",
							"Value": "{isAdmin}",
							"Inline": true
						},
						{
							"Name": "Created on",
							"Value": "{acc-created}",
							"Inline": true
						},
						{
							"Name": "Joined on",
							"Value": "{join-on}",
							"Inline": true
						},
						{
							"Name": "No of Roles",
